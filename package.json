{
  "name": "animals-in-space",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.2.0",
    "apollo-server-lambda": "^2.6.9",
    "bufferutil": "^4.0.1",
    "dotenv": "^8.0.0",
    "encoding": "^0.1.12",
    "graphql": "^14.4.2",
    "netlify-lambda": "^1.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "utf-8-validate": "^5.0.2"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "http-proxy-middleware": "^0.19.1",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "concurrently 'npm run start:ui' 'npm run start:lambda'",
    "start:ui": "react-scripts start",
    "start:lambda": "NODE_ENV=development netlify-lambda serve src/lambda",
    "build": "npm run build:app && npm run build:lambda",
    "build:app": "react-scripts build",
    "build:lambda": "netlify-lambda build src/lambda",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
